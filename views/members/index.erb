<h1>Members</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th>Email Address</th>
    <th></th>
  </tr>
  <% for member in @members %>
  <tr>
    <td><%= member.name %></td>
    <td>|</td>
    <td><%= member.email %></td>
    <td>|</td>
    <td><a href="/members/<%= member.id %>" >View</a></td>
  </tr>
  <% end %>
</table>
